# Property Owner Extraction Pipeline - Environment Variables
# Copy this file to .env and configure as needed

# Debug Mode
# Set to True for verbose logging
DEBUG=False

# Logging Level
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Output Format
# Options: excel, csv, json
OUTPUT_FORMAT=excel

# Enable/Disable Scraping
# Set to False to disable Orange County web scraping
ENABLE_SCRAPING=True

# Wake County API Configuration (Optional Overrides)
# WAKE_API_BASE_URL=https://services.wake.gov/realestate/api/
# WAKE_API_MAX_RECORDS=1000

# Orange County Scraper Configuration (Optional Overrides)
# ORANGE_SCRAPER_HEADLESS=True
# ORANGE_SCRAPER_MAX_RECORDS=300

# Rate Limiting (Optional Overrides)
# API_DELAY_SECONDS=0.5
# SCRAPER_DELAY_SECONDS=2.5

# Checkpoint Configuration
# ENABLE_CHECKPOINTS=True
# CHECKPOINT_FREQUENCY=100

# Quality Score Thresholds (Optional Overrides)
# HIGH_QUALITY_THRESHOLD=80
# MEDIUM_QUALITY_THRESHOLD=50
